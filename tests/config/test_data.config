// def nf_core_scrnaseq_data = 'https://github.com/nf-core/test-datasets/raw/scrnaseq'
def nf_core_scrnaseq_data = 'https://github.com/fbdtemme/test-datasets/raw/scrnaseq'
 

params {
    test_data_scrnaseq {
        samplesheet = "${nf_core_scrnaseq_data}/samplesheet.csv"

        testdata {
            R1 = "${nf_core_scrnaseq_data}/testdata/S10_L001_R1_001.fastq.gz"
            R2 = "${nf_core_scrnaseq_data}/testdata/S10_L001_R2_001.fastq.gz"
        }

        reference {
            mouse_genome              = "${nf_core_scrnaseq_data}/reference/GRCm38.p6.genome.chr19.fa"
            mouse_transcriptome       = "${nf_core_scrnaseq_data}/reference/GRCm38.p6.genome.chr19.transcriptome.fa"
            mouse_gtf                 = "${nf_core_scrnaseq_data}/reference/gencode.vM19.annotation.chr19.gtf"
            tenx_V2_barcode_whitelist = "${nf_core_scrnaseq_data}/reference/10xv2_whitelist_sub50K.txt"
            mouse_tx2gene             = "${nf_core_scrnaseq_data}/reference/gencode.vM19.annotation.chr19.tx2gene.gtf"
        }

        results {
            alevin                   = "${nf_core_scrnaseq_data}/results/S10_L001_alevin_results.tar.gz"
        }
    }
}

includeConfig 'https://github.com/nf-core/modules/raw/master/tests/config/test_data.config'
